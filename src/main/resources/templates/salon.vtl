<h1>View the Stylists</h1>
#if ($stylists.size() > 0) 
 <div class="row">
 	#foreach ($stylist in $stylists)
      <div class="col-md-4">
      	<h3>Name:</h3>
      	<p class="space">$stylist.getName()</p>
      	<h3>Style:</h3>
      	<p class="space">$stylist.getStyle()</p>
      	<h3>Skills:</h3>
      	<p class="space">$stylist.getSkills()</p>
      </div>
 	#end
 </div>
#else 
  <h3>need to add more stylist</h3>
#end